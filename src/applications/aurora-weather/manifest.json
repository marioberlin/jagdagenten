{
  "id": "aurora-weather",
  "name": "Aurora Weather",
  "version": "1.0.0",
  "description": "Real-time weather intelligence powered by A2A agents",
  "author": "LiquidCrypto",
  "license": "MIT",
  "category": "weather",
  "keywords": ["weather", "forecast", "aurora", "a2a", "intelligence"],
  "icon": "CloudSun",

  "entry": "./App.tsx",

  "window": {
    "mode": "floating",
    "title": "Aurora Weather",
    "defaultSize": { "width": 900, "height": 600 },
    "resizable": true
  },

  "integrations": {
    "dock": {
      "enabled": true,
      "position": 7
    },
    "aiContext": {
      "systemPrompt": "You are a weather intelligence assistant providing real-time forecasts and weather analysis.",
      "agentId": "aurora-weather-agent"
    }
  },

  "capabilities": [
    "network:http",
    "a2a:connect",
    "storage:local",
    "notification:toast",
    "media:geolocation"
  ]
}
