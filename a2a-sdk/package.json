{
  "name": "@liquidcrypto/a2a-sdk",
  "version": "0.1.0",
  "description": "A TypeScript SDK for the Agent2Agent (A2A) Protocol",
  "license": "Apache-2.0",
  "author": "A2A Project",
  "keywords": [
    "A2A",
    "A2A SDK",
    "A2A Protocol",
    "Agent2Agent",
    "Agent 2 Agent"
  ],
  "homepage": "https://a2a-protocol.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/a2aproject/a2a-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/a2aproject/a2a-typescript/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:docs": "typedoc",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "prepublishOnly": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "example:server": "ts-node examples/server-example.ts",
    "example:client": "ts-node examples/client-example.ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests",
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "google-protobuf": "^3.21.0",
    "pino": "^9.0.0",
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "@fastify/cors": "^9.0.0",
    "@fastify/sse-v2": "^3.1.0",
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.10",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/instrumentation": "^0.49.0",
    "@opentelemetry/sdk-node": "^0.49.0",
    "axios": "^1.6.0",
    "better-sqlite3": "^9.4.0",
    "eventsource": "^2.0.0",
    "express": "^4.18.0",
    "fastify": "^4.25.0",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.7.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.29",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "playwright": "^1.57.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0"
  }
}