{
  "name": "@liquidcrypto/a2a-sdk",
  "version": "0.2.0",
  "description": "A2A Protocol SDK for LiquidCrypto - Agent-to-Agent communication with A2A v1.0 compliance",
  "license": "Apache-2.0",
  "author": "LiquidCrypto",
  "keywords": [
    "A2A",
    "A2A SDK",
    "A2A Protocol",
    "Agent2Agent",
    "LiquidCrypto"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./types/v1": {
      "types": "./dist/types/v1.d.ts",
      "import": "./dist/types/v1.js"
    },
    "./compat": {
      "types": "./dist/compat/index.d.ts",
      "import": "./dist/compat/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "pino": "^9.0.0",
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.10",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/instrumentation": "^0.49.0",
    "@opentelemetry/sdk-node": "^0.49.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.29",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
